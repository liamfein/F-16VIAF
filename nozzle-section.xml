<?xml version="1.0"?>

<PropertyList>

 <path>nozzle-section.ac</path>

 <effect>
      <inherits-from>Aircraft/f16/Models/Effects/fuselage/model-combined-fuselage</inherits-from>
      <parameters>
          <metallic type="float">0.90</metallic>
          <ambient-factor type="float">0.00</ambient-factor>
          <rain-enabled type="int">0</rain-enabled>
          <snow-enabled type="int">0</snow-enabled>
          <ambient-correction type="float">0.00</ambient-correction>
          <reflection-enabled type="int">1</reflection-enabled>
          <reflection-correction type="float">-0.46</reflection-correction>
          <specular-material-ignore type="int">0</specular-material-ignore>
      </parameters>
      <!-- outer plates -->
      <object-name>us1</object-name>
  </effect>
  
  <effect>
      <inherits-from>Aircraft/f16/Models/Effects/fuselage/model-combined-fuselage</inherits-from>
      <parameters>
          <metallic type="float">0.90</metallic>
          <ambient-factor type="float">0.00</ambient-factor>
          <rain-enabled type="int">0</rain-enabled>
          <snow-enabled type="int">0</snow-enabled>
          <ambient-correction type="float">0.00</ambient-correction>
          <reflection-enabled type="int">0</reflection-enabled>
          <specular-material-ignore type="int">0</specular-material-ignore>
      </parameters>
      <!-- inner plates -->
      <object-name>ls1</object-name>
      <object-name>ls2</object-name>
  </effect>

 <animation>
  <type>rotate</type>
  <object-name>us1</object-name>
  <property>sim/multiplay/generic/float[0]</property>
  <offset-deg>-5</offset-deg>
  <factor>10</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.49</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ls1</object-name>
  <property>sim/multiplay/generic/float[0]</property>
  <offset-deg>-5</offset-deg>
  <factor>10</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>0.0</y-m>
   <z-m>0.45</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>ls2</object-name>
  <property>sim/multiplay/generic/float[0]</property>
  <offset-deg>-5</offset-deg>
  <factor>10</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>-0.115</y-m>
   <z-m>0.435</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>-0.948683</y>
   <z>-0.316228</z>
  </axis>
 </animation>

</PropertyList>

<?xml version="1.0"?>

<PropertyList>

 <path>pilot_externalview.ac</path>

 <animation>
        <type>material</type>
        <object-name>helmet</object-name>
        <object-name>patchL</object-name>
        <object-name>patchR</object-name>
        <object-name>patchC</object-name>
        <property-base>sim/model/livery-patch</property-base>
        <texture-prop>texture</texture-prop>
        <texture>Liveries/patch.default.png</texture>
    </animation>
    
    <effect>
      <inherits-from>Effects/model-combined-transparent</inherits-from>
      <parameters>
			<rain-enabled type="int">0</rain-enabled>
			<snow-enabled type="int">0</snow-enabled>
			<ambient-correction type="float">0.00</ambient-correction>
			<lightmap-enabled type="int">0</lightmap-enabled>
			<reflection-enabled type="int">1</reflection-enabled>			
			<reflection-correction type="float">0.50</reflection-correction>
			<reflection-fresnel type="float">0.01</reflection-fresnel>
			<reflection-fresnel-factor type="float">0.01</reflection-fresnel-factor>
			<reflection-rainbow type="float">0.35</reflection-rainbow>
			<reflection-noise type="float">0.0</reflection-noise>
			<reflection-dynamic type="int">1</reflection-dynamic>
			<reflection-type type="int">1</reflection-type>
			<texture n= "0" ><type>black</type></texture>
			<texture n= "5" >
				<type>cubemap</type>
				<images>
				  <positive-x>Aircraft/Generic/Effects/fgfs-sky2/1.png</positive-x>
				  <negative-x>Aircraft/Generic/Effects/fgfs-sky2/4.png</negative-x>
				  <positive-y>Aircraft/Generic/Effects/fgfs-sky2/2.png</positive-y>
				  <negative-y>Aircraft/Generic/Effects/fgfs-sky2/3.png</negative-y>
				  <positive-z>Aircraft/Generic/Effects/fgfs-sky2/6.png</positive-z>
				  <negative-z>Aircraft/Generic/Effects/fgfs-sky2/5.png</negative-z>
				</images>
			</texture>
			<transparent>true</transparent>
			<render-bin>
			  <bin-number>111</bin-number>
			  <bin-name>DepthSortedBin</bin-name>
			</render-bin>
      </parameters>
      <object-name>L-visor</object-name>
 </effect>
 
 	<effect>
      	<inherits-from>Effects/model-combined-deferred</inherits-from>
      	<parameters>
			<rain-enabled type="int">0</rain-enabled>
			<snow-enabled type="int">0</snow-enabled>
			<ambient-correction type="float">0.00</ambient-correction>
			<lightmap-enabled type="int">0</lightmap-enabled>
			<reflection-enabled type="int">0</reflection-enabled>	
			<normalmap-enabled type="int">0</normalmap-enabled>
			<lightmap-enabled type="int">0</lightmap-enabled>
			<lightmap-multi type="int">0</lightmap-multi>
			<darkmap-enabled type="int">0</darkmap-enabled>
			<reflect-map-enabled type="int">0</reflect-map-enabled>
			<grain-texture-enabled type="int">0</grain-texture-enabled>
     	</parameters>
        <object-name>helmet</object-name>
        <object-name>patchL</object-name>
        <object-name>patchR</object-name>
        <object-name>patchC</object-name>        
		<object-name>eye-balls</object-name>
		
		<object-name>hand.001</object-name>
		<object-name>thumb.001</object-name>
		<object-name>LL-arm1</object-name>
		<object-name>LL-arm2</object-name>
		<object-name>LL-wrist</object-name>
		<object-name>LL-Boot</object-name>
		<object-name>LL-leg1</object-name>
		<object-name>LL-thigh</object-name>
		<object-name>hand</object-name>
		<object-name>thumb</object-name>
		<object-name>LR-wrist</object-name>
		<object-name>LR-arm2</object-name>
		<object-name>LR-arm1</object-name>
		<object-name>LR-Boot</object-name>
		<object-name>LR-leg1</object-name>
		<object-name>LR-tigh</object-name>
		<object-name>L-chest</object-name>
		<object-name>L-collar</object-name>
		<object-name>L-harness</object-name>
		<object-name>L-mask-tube</object-name>
		<object-name>L-oxy-tube</object-name>
		<object-name>cylinder</object-name>
		<object-name>cylinder.001</object-name>
		<object-name>disk.001</object-name>
		<object-name>object</object-name>
		<object-name>L-head</object-name>
		<object-name>bag</object-name>
		<object-name>life-jacket</object-name>
		<object-name>disk</object-name>

   </effect>

   <animation>
     <type>select</type>
     <object-name>mount</object-name>
     <object-name>paper</object-name>
     <object-name>springFast</object-name>
     <object-name>Torus1</object-name>
     <object-name>Torus2</object-name>
     <object-name>Torus3</object-name>
     <object-name>Torus4</object-name>
     <condition>
        <equals>
          <property>f16/cockpit/kneeboard-left-visible</property>
          <value>1</value>
        </equals>
      </condition>
   </animation>

   <animation>
     <type>select</type>
     <object-name>mountr</object-name>
     <object-name>paper-r</object-name>
     <object-name>springFastr</object-name>
     <object-name>Torus1r</object-name>
     <object-name>Torus2r</object-name>
     <object-name>Torus3r</object-name>
     <object-name>Torus4r</object-name>
     <condition>
        <equals>
          <property>f16/cockpit/kneeboard-right-visible</property>
          <value>1</value>
        </equals>
      </condition>
   </animation>

</PropertyList>
